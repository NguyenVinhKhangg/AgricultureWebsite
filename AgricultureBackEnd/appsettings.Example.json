{
  "ConnectionStrings": {
    "DefaultConnection": "server=YOUR_SERVER; database=YOUR_DATABASE; uid=YOUR_USERNAME; pwd=YOUR_PASSWORD; TrustServerCertificate=True"
  },
  "JwtSettings": {
    "Key": "YOUR_SECRET_KEY_WITH_AT_LEAST_32_CHARACTERS",
    "Issuer": "AgricultureStoreAPI",
    "Audience": "AgricultureStoreClient",
    "ExpirationMinutes": 60
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SenderEmail": "YOUR_EMAIL@gmail.com",
    "SenderName": "Agriculture Store",
    "Username": "YOUR_EMAIL@gmail.com",
    "Password": "YOUR_APP_PASSWORD",
    "EnableSsl": true,
    "BaseUrl": "http://localhost:3000"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://yourdomain.com",
      "https://admin.yourdomain.com"
    ]
  }
}
